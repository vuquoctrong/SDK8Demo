<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Demo2602"
        android:name="com.example.demo2602.MainApplication"
        tools:targetApi="31" >
        <activity
            android:name="com.example.demo2602.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="APP_UUID"
            android:value="@string/string_jf_app_uuid" />
        <meta-data
            android:name="APP_KEY"
            android:value="@string/string_jf_app_key" />
        <meta-data
            android:name="APP_SECRET"
            android:value="@string/string_jf_app_secret" />
        <meta-data
            android:name="APP_MOVECARD"
            android:value="@string/string_jf_app_movecard" />

        <meta-data
            android:name="SDK_VHOME_APP_KEY"
            android:value="@string/string_SDK_VHOME_APP_KEY" />

        <meta-data
            android:name="SDK_VHOME_APP_SECKEY"
            android:value="@string/string_SDK_VHOME_APP_SECKEY" />

        <meta-data
            android:name="SDK_VHOME_BASE_URL"
            android:value="@string/string_SDK_VHOME_BASE_URL" />

        <meta-data
            android:name="SDK_VHOME_APP_NAME"
            android:value="@string/string_SDK_VHOME_APP_NAME" />
    </application>

</manifest>